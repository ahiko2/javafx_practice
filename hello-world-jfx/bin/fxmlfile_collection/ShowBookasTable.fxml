<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="413.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ShowBookasTableController">

    <TableView fx:id="bookTable" prefHeight="359.0" prefWidth="607.0">
        <columns>
            <TableColumn fx:id="noCol" prefWidth="50.0" text="No" />
            <TableColumn fx:id="isbnCol" text="ISBN" />
            <TableColumn fx:id="titleCol" text="Title" />
            <TableColumn fx:id="authorCol" text="Author" />
            <TableColumn fx:id="publishedDateCol" prefWidth="148.6666259765625" text="Published Date" />
            <TableColumn fx:id="timestampCol" prefWidth="167.33322143554688" text="Timestamp" />
        </columns>
    </TableView>

    <Button layoutX="448.0" layoutY="371.0" onAction="#showBookTable" text="Show Book List" />
    <Button fx:id="closeButton" layoutX="558.0" layoutY="371.0" onAction="#handleCloseButtonAction" text="Exit" />

</AnchorPane>